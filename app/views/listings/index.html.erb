<p id="notice"><%= notice %></p>

<h1>Listings</h1>
<%= render 'filter' %>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Descripttion</th>
      <th>Condition</th>
      <th>Price</th>
      <th>Sold</th>
      <th>Listing type</th>
    </tr>
  </thead>

  <tbody>
    <% @listings.each do |listing| %>
      <tr>
        <td><%= listing.title %></td>
        <td><%= listing.description %></td>
        <td><%= listing.condition %></td>
        <td><%= listing.price %></td>
        <td><%= listing.sold %></td>
        <td><%= listing.listing_type %></td>
        <td><%= link_to 'Show', listing %></td>
    <% end %>
  </tbody>
</table>


